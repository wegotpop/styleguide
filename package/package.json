{
  "name": "@wegotpop/styleguide",
  "version": "2.0.0",
  "license": "MIT",
  "files": [
    "*.css",
    "*.css.map"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wegotpop/styleguide.git"
  },
  "scripts": {
    "compile": "postcss -m -u postcss-partial-import -u postcss-modules -u cssnano -d ./ ../css/*.css",
    "prepare": "yarn compile"
  },
  "devDependencies": {
    "cssnano": "^4.1.0",
    "postcss": "^7.0.2",
    "postcss-cli": "^6.0.0",
    "postcss-modules": "^1.1.0",
    "postcss-partial-import": "^4.1.0"
  }
}
